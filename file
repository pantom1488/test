echo "osascript -e \"set volume 100\" && alias cd=\"say -v Yuri Перехожу в директорию && cd\"" >> ~/.zshrc
echo "osascript -e \"set volume 100\" && alias vim=\"say -v Yuri Вим запускается && vim\"" >> ~/.zshrc
echo "osascript -e \"set volume 100\" && alias ls=\"say -v Yuri Вывожу список файлов && ls\"" >> ~/.zshrc
echo "osascript -e \"set volume 100\" && alias norminette=\"say -v Yuri Что по норме && norminette\"" >> ~/.zshrc
source ~/.zshrc
