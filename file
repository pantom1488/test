echo "alias cd=\"say -v Yuri Перехожу в директорию && cd\"" >> ~/.zshrc
echo "alias vim=\"say -v Yuri Вим запускается && vim\"" >> ~/.zshrc
echo "alias ls=\"say -v Yuri Вывожу список файлов && ls\"" >> ~/.zshrc
echo "alias norminette=\"say -v Yuri Что по норме && norminette\"" >> ~/.zshrc
source ~/.zshrc

